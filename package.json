{
  "name": "tequila-client",
  "license": "MIT",
  "scripts": {
    "ios": "yarn workspace @kiwicom/margarita-mobile ios",
    "android": "yarn workspace @kiwicom/margarita-mobile android",
    "dev": "yarn workspace @kiwicom/margarita-web dev",
    "build": "yarn workspace @kiwicom/margarita-web build",
    "start": "yarn workspace @kiwicom/margarita-web start",
    "analyze": "yarn workspace @kiwicom/margarita-web analyze",
    "relay": "yarn babel-node --config=babel.config.js scripts/relay-compiler.js",
    "lint": "eslint . --report-unused-disable-directives",
    "flow": "flow --include-warnings",
    "test": "node node_modules/jest/bin/jest.js --config=.jest.json --silent",
    "test-ci": "yarn flow && yarn lint && yarn test",
    "server": "yarn workspace @kiwicom/margarita-graphql start"
  },
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "dependencies": {
    "@kiwicom/margarita-relay": "^0",
    "cross-env": "^5.2.0",
    "expo": "^31.0.6",
    "next": "^7.0.2",
    "next-plugin-transpile-modules": "^0.1.3",
    "react": "^16.6.3",
    "react-art": "^16.5.2",
    "react-dom": "^16.6.3",
    "react-native": "https://github.com/expo/react-native/archive/sdk-31.0.0.tar.gz",
    "react-native-web": "^0.9.3",
    "react-navigation": "^3.0.6",
    "schedule": "^0.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/node": "^7.0.0",
    "@kiwicom/eslint-config": "^0.3.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-relay": "^1.7.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.0",
    "flow-bin": "^0.86.0",
    "jest": "^23.6.0",
    "jest-fetch-mock": "^2.0.1",
    "prettier": "^1.15.2",
    "react-native-testing-library": "^1.4.2",
    "react-test-renderer": "^16.6.3",
    "relay-compiler": "^1.7.0"
  },
  "resolutions": {
    "**/babel-core": "7.0.0-bridge.0"
  }
}