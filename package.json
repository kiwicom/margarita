{
  "name": "tequila-client",
  "license": "MIT",
  "scripts": {
    "ios": "expo start --ios",
    "android": "expo start --android",
    "dev": "yarn workspace @kiwicom/margarita-web dev",
    "build": "yarn workspace @kiwicom/margarita-web build",
    "start": "yarn workspace @kiwicom/margarita-web start",
    "relay": "yarn babel-node --config=babel.config.js scripts/relay-compiler.js",
    "lint": "eslint . --report-unused-disable-directives",
    "flow": "flow --include-warnings",
    "analyze": "yarn workspace @kiwicom/margarita-web analyze",
    "test": "node node_modules/jest/bin/jest.js --config=.jest.json",
    "test-ci": "yarn flow && yarn lint && yarn test",
    "graphql": "yarn babel-node scripts/createSchema.js"
  },
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "cross-env": "^5.2.0",
    "react": "^16.6.3",
    "react-art": "^16.5.2",
    "react-dom": "^16.6.3",
    "schedule": "^0.4.0"

  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/node": "^7.0.0",
    "@kiwicom/eslint-config": "^0.3.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-relay": "^1.7.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.0",
    "flow-bin": "^0.86.0",
    "jest": "^23.6.0",
    "prettier": "^1.15.2",
    "react-native-dotenv": "^0.2.0",
    "react-native-testing-library": "^1.4.2",
    "react-test-renderer": "^16.6.3",
    "relay-compiler": "^1.7.0",
    "webpack": "^4.26.1",
    "webpack-bundle-analyzer": "^3.0.3"
  },
  "resolutions": {
    "**/babel-core": "7.0.0-bridge.0"
  }
}
