{
  "preset": "jest-expo",
  "testPathIgnorePatterns": [
    "<rootDir>/.next/",
    "<rootDir>/node_modules/",
    "<rootDir>/.expo/",
    "__tests__/__mocks__"
  ],
  "setupTestFrameworkScriptFile": "<rootDir>/scripts/setupTests.js",
  "transformIgnorePatterns": [
    "/node_modules/(?!@kiwicom)/"
  ]
}